<!-- ユーザーの詳細 -->
<div class='container py-5 px-sm-0'>
  <div class='row'>
      <div class='col-md-3'>
      <!-- ユーザープロフィール-->
      <%= render 'admin/users/info', user: @user %>
    </div>

    <div class='col-md-8 offset-md-1'>
      <table class="table table-user">
      <%= render 'admin/items/postitems', items: @items %>
    </div>
  </div>
  <div class="row justify-content-center">
    <%= paginate @items %>
  </div>
</div>